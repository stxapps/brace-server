{
  "name": "brace-server",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": "16.x.x"
  },
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node src/index.js",
    "start-local": "GOOGLE_APPLICATION_CREDENTIALS=~/.config/gcloud/legacy_credentials/brace@brace.to/adc.json nodemon --experimental-specifier-resolution=node src/index.js",
